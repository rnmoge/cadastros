<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="CadastrosBaseService"
                   namespace="CadastrosBaseService.Model" default-lazy="false">
  <class name="NcmDTO" table="ncm">
    <id name="Id" column="ID">
      <generator class="sequence">
        <param name="sequence">ncm_id_seq</param>
      </generator>
    </id>
    <property name="Codigo" column="CODIGO"/>
    <property name="Descricao" column="DESCRICAO"/>
    <property name="Observacao" column="OBSERVACAO"/>
  </class>
</hibernate-mapping>
