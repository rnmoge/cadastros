<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
       assembly="CadastrosBaseService"
       namespace="CadastrosBaseService.Model" default-lazy="false">
<class name="SalarioMinimoDTO" table="salario_minimo">
   <id name="id" column="ID">
     <generator class="sequence">
       <param name="sequence">salario_minimo_id_seq</param>
     </generator>
    </id>
    <property name="vigencia" column="VIGENCIA"/>
    <property name="valor_mensal" column="VALOR_MENSAL"/>
    <property name="valor_diario" column="VALOR_DIARIO"/>
    <property name="valor_hora" column="VALOR_HORA"/>
    <property name="norma_legal" column="NORMA_LEGAL"/>
    <property name="dou" column="DOU"/>
  </class>
</hibernate-mapping>
