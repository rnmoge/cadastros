    <property name="Descricao" column="DESCRICAO" />
    <property name="GrauInstrucaoCaged" column="GRAU_INSTRUCAO_CAGED" />
    <property name="GrauInstrucaoSefip" column="GRAU_INSTRUCAO_SEFIP" />
