<?xml version="1.0" encoding="utf-8"?>
<UserControl x:Class="CadastrosBaseClient.View.CadastrosBase.Endereco" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" mc:Ignorable="d" d:DesignHeight="500" d:DesignWidth="700">
	<DockPanel LastChildFill="True">
		<StackPanel DockPanel.Dock="Top" HorizontalAlignment="Left" Orientation="Horizontal">
			<Button Name="btGravar" Width="70" Height="20">
				<StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
					<TextBlock Text="Salvar" Margin="3,0,0,0" VerticalAlignment="Center" />
				</StackPanel>
			</Button>
			<Button Name="btSair" Width="70" Height="20">
				<StackPanel Orientation="Horizontal">
					<TextBlock Text="Voltar" Margin="3,0,0,0" VerticalAlignment="Center" />
				</StackPanel>
			</Button>
		</StackPanel>
		<Grid>
            <TextBox HorizontalAlignment="Left" VerticalAlignment="Top" Text="{Binding EnderecoSelected.Logradouro}" Height="23" Width="582" Margin="6,25,0,0"  />
			<Label Content="Logradouro" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="6,6,0,0"  />
			<TextBox HorizontalAlignment="Left" VerticalAlignment="Top" Text="{Binding EnderecoSelected.Numero}" Height="23" Width="100" Margin="594,25,0,0"  />
			<Label Content="Numero" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="594,6,0,0"  />
			<TextBox HorizontalAlignment="Left" VerticalAlignment="Top" Text="{Binding EnderecoSelected.Complemento}" Height="23" Width="688" Margin="6,75,0,0"  />
			<Label Content="Complemento" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="6,56,0,0"  />
			<TextBox HorizontalAlignment="Left" VerticalAlignment="Top" Text="{Binding EnderecoSelected.Bairro}" Height="23" Width="100" Margin="6,126,0,0"  />
			<Label Content="Bairro" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="6,107,0,0"  />
			<TextBox HorizontalAlignment="Left" VerticalAlignment="Top" Text="{Binding EnderecoSelected.Cidade}" Height="23" Width="100" Margin="112,126,0,0"  />
			<Label Content="Cidade" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="112,107,0,0"  />
			<TextBox HorizontalAlignment="Left" VerticalAlignment="Top" Text="{Binding EnderecoSelected.Cep}" Height="23" Width="100" Margin="218,126,0,0"  />
			<Label Content="Cep" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="218,107,0,0"  />
			<TextBox HorizontalAlignment="Left" VerticalAlignment="Top" Text="{Binding EnderecoSelected.MunicipioIbge}" Height="23" Width="100" Margin="324,126,0,0"  />
			<Label Content="MunicipioIbge" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="324,107,0,0"  />
			<ComboBox HorizontalAlignment="Left" VerticalAlignment="Top" Height="23" Width="100" SelectedValue="{Binding EnderecoSelected.Uf}" SelectedValuePath="Tag" Margin="430,126,0,0" >
				<ComboBoxItem Content="Sim" Tag="S" />
				<ComboBoxItem Content="Não" Tag="N" />
			</ComboBox>
			<Label Content="Uf" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="430,107,0,0"  />
			<TextBox HorizontalAlignment="Left" VerticalAlignment="Top" Text="{Binding EnderecoSelected.Fone}" Height="23" Width="100" Margin="536,126,0,0"  />
			<Label Content="Fone" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="536,107,0,0"  />
			<TextBox HorizontalAlignment="Left" VerticalAlignment="Top" Text="{Binding EnderecoSelected.Fax}" Height="23" Width="100" Margin="6,174,0,0"  />
			<Label Content="Fax" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="6,155,0,0"  />
			<ComboBox HorizontalAlignment="Left" VerticalAlignment="Top" Height="23" Width="100" SelectedValue="{Binding EnderecoSelected.Principal}" SelectedValuePath="Tag" Margin="112,174,0,0" >
				<ComboBoxItem Content="Sim" Tag="S" />
				<ComboBoxItem Content="Não" Tag="N" />
			</ComboBox>
			<Label Content="Principal" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="112,155,0,0"  />
			<ComboBox HorizontalAlignment="Left" VerticalAlignment="Top" Height="23" Width="100" SelectedValue="{Binding EnderecoSelected.Entrega}" SelectedValuePath="Tag" Margin="218,174,0,0" >
				<ComboBoxItem Content="Sim" Tag="S" />
				<ComboBoxItem Content="Não" Tag="N" />
			</ComboBox>
			<Label Content="Entrega" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="218,155,0,0"  />
			<ComboBox HorizontalAlignment="Left" VerticalAlignment="Top" Height="23" Width="100" SelectedValue="{Binding EnderecoSelected.Cobranca}" SelectedValuePath="Tag" Margin="324,174,0,0" >
				<ComboBoxItem Content="Sim" Tag="S" />
				<ComboBoxItem Content="Não" Tag="N" />
			</ComboBox>
			<Label Content="Cobranca" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="324,155,0,0"  />
			<ComboBox HorizontalAlignment="Left" VerticalAlignment="Top" Height="23" Width="100" SelectedValue="{Binding EnderecoSelected.Correspondencia}" SelectedValuePath="Tag" Margin="430,174,0,0" >
				<ComboBoxItem Content="Sim" Tag="S" />
				<ComboBoxItem Content="Não" Tag="N" />
			</ComboBox>
			<Label Content="Correspondencia" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="430,155,0,0"  />
		</Grid>
	</DockPanel>
</UserControl>