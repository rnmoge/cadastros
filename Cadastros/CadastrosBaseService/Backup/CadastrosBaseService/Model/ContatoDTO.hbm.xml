<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="CadastrosBaseService" namespace="CadastrosBaseService.Model" default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
  <class name="ContatoDTO" table="contato" >
    <id name="Id" column="ID">
      <generator class="identity" />
    </id>
    <property name="IdPessoa" column="ID_PESSOA" />
    <property name="IdEmpresa" column="ID_EMPRESA" />
    <property name="Nome" column="NOME" />
    <property name="Email" column="EMAIL" />
    <property name="FoneComercial" column="FONE_COMERCIAL" />
    <property name="FoneResidencial" column="FONE_RESIDENCIAL" />
    <property name="FoneCelular" column="FONE_CELULAR" />
  </class>
</hibernate-mapping>